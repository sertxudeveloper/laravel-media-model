{
    "name": "sertxudeveloper/laravel-media-model",
    "description": "Attach media files to your models",
    "keywords": [
        "sertxudeveloper",
        "laravel",
        "laravel-media-model"
    ],
    "homepage": "https://github.com/sertxudeveloper/laravel-media-model",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Peris",
            "email": "dev.sertxu@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SertxuDeveloper\\Media\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SertxuDeveloper\\Media\\Tests\\": "tests",
            "SertxuDeveloper\\Media\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SertxuDeveloper\\Media\\MediaServiceProvider"
            ],
            "aliases": {
                "Media": "SertxuDeveloper\\Media\\Facades\\Media"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
